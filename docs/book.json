{
  "title": "SiteServer CMS 表单插件",
  "description": "SiteServer CMS 表单插件可以管理多个表单，还可以通过简单的标记灵活地自定义表单样式，同时表单插件支持数据验证、Ajax提交、验证码等功能。",
  "author": "SiteServer CMS",
  "output.name": "site",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "readme.md"
  },
  "links": {
    "sidebar": {
      "SiteServer CMS 表单插件": "index.html"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "ga",
    "-sharing",
    "sharing-plus",
    "favicon",
    "splitter",
    "tbfed-pagefooter",
    "multipart",
    "edit-link"
  ],
  "pluginsConfig": {
    "sharing": {
      "douban": true,
      "linkedin": true,
      "qq": true,
      "qzone": true,
      "weibo": true,
      "all": [
        "douban",
        "weibo",
        "linkedin",
        "qq",
        "qzone"
      ]
    },
    "ga": {
      "token": "UA-1398374-1"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico",
      "appleTouch": "assets/favicon.ico",
      "appleTouchMore": {
        "120x120": "assets/favicon.ico",
        "180x180": "assets/favicon.ico"
      }
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy <a href='http://www.siteserver.cn'>北京百容千域软件技术开发有限责任公司</a> 2018",
      "modify_label": "修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": false
    },
    "edit-link": {
      "base": "https://github.com/siteserver/ss.form/edit/dev/docs",
      "label": "编辑此页面"
    }
  }
}